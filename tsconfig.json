{
    "compilerOptions": {
        "module":"commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": false,
        "declaration": true,
        "experimentalDecorators": false,
        "noEmitOnError": true,
        "lib":["es6"]
    },
    "exclude": [
        "node_modules",
        "bin"
    ]
}